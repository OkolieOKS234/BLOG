<%-include("partials/header")-%>
<div class="container-fluid">
  <h1>Compose</h1>
  <form action="/" method="post">
    <div class="form-group">
      <label for="title">Title</label>
      <input type="text" name="postTitle" class="form-control" />
    </div>
    <div class="form-group">
      <label for="">POST</label>
      <textarea
